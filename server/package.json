{
  "name": "travel-blog",
  "version": "1.0.0",
  "description": "Сайт о путешествиях",
  "main": "Bootstrap.js",
  "scripts": {
    "dev": "tsnd ./src/Bootstrap.ts",
    "build": "tsc",
    "start": "node ./dist/Bootstrap.js",
    "dcr:up": "docker-compose --env-file .env.development -f ../docker-compose.yml -f ../docker-compose.override.yml up",
