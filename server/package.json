    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "inversify-inject-decorators": "^3.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3",
    "winston": "^3.8.1"
